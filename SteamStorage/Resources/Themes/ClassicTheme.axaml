<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush Color="#0F141A" x:Key="BackgroundBrush" />
    <SolidColorBrush Color="#252C32" x:Key="BackgroundAdditionalBrush" />
    <SolidColorBrush Color="#D2DADD" x:Key="ForegroundBrush" />
    <SolidColorBrush Color="#5F34B9" x:Key="FirstAccentBrush" />
    <SolidColorBrush Color="#9F4DC9" x:Key="SecondAccentBrush" />
    <SolidColorBrush Color="#3E7FC5" x:Key="ThirdAccentBrush" />

    <SolidColorBrush Color="#363E44" x:Key="TopLevelBackgroundBrush" />
    <SolidColorBrush
        Color="#898C8F"
        Opacity="0.25"
        x:Key="TopLevelBackgroundLightBrush" />

    <SolidColorBrush Color="#1E9538" x:Key="PositiveBrush" />
    <SolidColorBrush Color="#E03737" x:Key="NegativeBrush" />

    <LinearGradientBrush x:Key="FirstAccentLinearBrush">
        <GradientStop Color="#6B51D9" Offset="0" />
        <GradientStop Color="#531C9C" Offset="0.75" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="SecondAccentLinearBrush">
        <GradientStop Color="#9F4DC9" Offset="0" />
        <GradientStop Color="#8C17B6" Offset="0.75" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ThirdAccentLinearBrush">
        <GradientStop Color="#3E7FC5" Offset="0" />
        <GradientStop Color="#1E62AB" Offset="0.75" />
    </LinearGradientBrush>

</ResourceDictionary>
