<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<SolidColorBrush x:Key="BackgroundBrush" Color="#0F141A"/>
	<SolidColorBrush x:Key="BackgroundAdditionalBrush" Color="#252C32"/>
	<SolidColorBrush x:Key="ForegroundBrush" Color="#D2DADD"/>
	<SolidColorBrush x:Key="FirstAccentBrush" Color="#5F34B9"/>
	<SolidColorBrush x:Key="SecondAccentBrush" Color="#9F4DC9"/>
	<SolidColorBrush x:Key="ThirdAccentBrush" Color="#3E7FC5"/>
	
	<SolidColorBrush x:Key="TopLevelBackgroundBrush" Color="#363E44"/>
	<SolidColorBrush x:Key="TopLevelBackgroundLightBrush" Color="#898C8F" Opacity="0.25"/>
	
	<SolidColorBrush x:Key="PositiveBrush" Color="#1E9538"/>
	<SolidColorBrush x:Key="NegativeBrush" Color="#E03737"/>
	
	<LinearGradientBrush x:Key="FirstAccentLinearBrush">
		<GradientStop Offset="0" Color="#6B51D9" />
		<GradientStop Offset="0.75" Color="#531C9C" />
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="SecondAccentLinearBrush">
		<GradientStop Offset="0" Color="#9F4DC9" />
		<GradientStop Offset="0.75" Color="#8C17B6" />
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="ThirdAccentLinearBrush">
		<GradientStop Offset="0" Color="#3E7FC5" />
		<GradientStop Offset="0.75" Color="#1E62AB" />
	</LinearGradientBrush>
	
</ResourceDictionary>
