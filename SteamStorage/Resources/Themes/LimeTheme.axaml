<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush Color="#0F141A" x:Key="BackgroundBrush" />
    <SolidColorBrush Color="#252C32" x:Key="BackgroundAdditionalBrush" />
    <SolidColorBrush Color="#089137" x:Key="ForegroundBrush" />
    <SolidColorBrush Color="#089137" x:Key="FirstAccentBrush" />
    <SolidColorBrush Color="#2D8553" x:Key="SecondAccentBrush" />
    <SolidColorBrush Color="#4DB991" x:Key="ThirdAccentBrush" />
    <SolidColorBrush Color="#4E9036" x:Key="FourthAccentBrush" />

    <SolidColorBrush Color="#232323" x:Key="TopLevelBackgroundBrush" />
    <SolidColorBrush
        Color="#FF1F1F"
        Opacity="0.25"
        x:Key="TopLevelBackgroundLightBrush" />

    <SolidColorBrush Color="#1E9538" x:Key="PositiveBrush" />
    <SolidColorBrush Color="#E03737" x:Key="NegativeBrush" />

    <LinearGradientBrush x:Key="FirstAccentLinearBrush">
        <GradientStop Color="#25BB59" Offset="0" />
        <GradientStop Color="#089037" Offset="0.75" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="SecondAccentLinearBrush">
        <GradientStop Color="#2D8553" Offset="0" />
        <GradientStop Color="#1D5736" Offset="0.75" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ThirdAccentLinearBrush">
        <GradientStop Color="#4DB991" Offset="0" />
        <GradientStop Color="#03A569" Offset="0.75" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="FourthAccentLinearBrush">
        <GradientStop Color="#4E9036" Offset="0" />
        <GradientStop Color="#29690B" Offset="0.75" />
    </LinearGradientBrush>

</ResourceDictionary>
